Package: fable.ata
Type: Package
Title: Fable Modelling Wrappers for ATAforecasting Package
Version: 0.0.1
Date: 2021-10-06
Authors@R: c(
  person("Ali Sabri", "Taylan", email="alisabritaylan@gmail.com", role=c("aut", "cre", "cph"), comment = c(ORCID = "0000-0001-9514-934X")),
